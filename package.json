{
  "name": "pelocal_voice_ai_agent",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "start:8009": "next start -p 8009 -H 0.0.0.0",
    "ws": "node ws-server.js",
    "ws:passthrough": "node ws-passthrough.js",
    "ws:pipeline": "node ws-server.js",
    "start:ws": "node ws-server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all || true",
    "pm2:delete": "pm2 delete all || true",
    "ws:test": "node test-exotel-protocol.js",
    "lint": "eslint app components hooks types --max-warnings 100",
    "lint:fix": "eslint app components hooks types --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles --no-cache",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "check:coverage": "node scripts/check-coverage.js 90",
    "test:api": "node run-tests.js --api-only",
    "test:e2e": "node run-tests.js --e2e-only",
    "test:ui": "node run-tests.js --ui-only",
    "test:components": "node test-components-static.js",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:unit:components": "jest --testPathPattern=__tests__/components",
    "test:unit:hooks": "jest --testPathPattern=__tests__/hooks",
    "test:unit:api": "jest --testPathPattern=__tests__/api",
    "check:endpoints": "node scripts/check-endpoints.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "csv-parse": "^6.1.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.19.1",
    "next": "15.5.4",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
