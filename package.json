{
  "name": "pelocal_voice_ai_agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint app components hooks types",
    "lint:fix": "eslint app components hooks types --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "check:coverage": "node scripts/check-coverage.js 90",
    "test:api": "node run-tests.js --api-only",
    "test:e2e": "node run-tests.js --e2e-only",
    "test:ui": "node run-tests.js --ui-only",
    "test:components": "node test-components-static.js",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:unit:components": "jest --testPathPattern=__tests__/components",
    "test:unit:hooks": "jest --testPathPattern=__tests__/hooks",
    "test:unit:api": "jest --testPathPattern=__tests__/api"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "csv-parse": "^6.1.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.19.1",
    "next": "15.5.4",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
